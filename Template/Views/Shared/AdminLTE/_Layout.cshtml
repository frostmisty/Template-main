<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1" />
    <meta http-equiv="x-ua-compatible" content="ie=edge" />

    <title>Template</title>
    <partial name="AdminLTE/_Styles" />
    <partial name="AdminLTE/_LinkCSS" />
</head>
<body class="hold-transition sidebar-mini layout-fixed">
    <div class="wrapper">
        <partial name="AdminLTE/_TopNavigation" />
        @(await Component.InvokeAsync("MainNavigation"))
        <div class="content-wrapper">
            @RenderBody()
        </div>
        <partial name="AdminLTE/_Footer" />
    </div>
    <partial name="AdminLTE/_Scripts" />

    <script type="text/javascript">
        $(function () {
            @if (IsSectionDefined("CustomJSFooter"))
            {
                @RenderSection("CustomJSFooter")
            }

        });
    </script>
</body>
</html>
